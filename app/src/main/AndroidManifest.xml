    android:theme="@style/AppTheme">
    <activity android:name=".Mahasiswa.DashboardMahasiswa">
    </activity>
    <activity android:name=".Welcome">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".Dosen.DashboardDosen"></activity>
</application>

</manifest>